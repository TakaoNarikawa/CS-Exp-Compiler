program IFELSE;
var a;
var b;
begin
   if a = b then
   begin
      if a = b then
      begin
         write(1)
      end
      else
      begin
         write(2)
      end
   end
   else
   begin
      write(3)
   end;
   if a <> b then
   begin
      if a = b then
      begin
         write(1)
      end
      else
      begin
         write(2)
      end
   end
   else
   begin
      write(3)
   end;
end.